@article{ WOS:000878646700001,
Author = {Xia, Qiudong and Ke, Lu'an and Zheng, Zheng},
Title = {Is video creation more effective than self-exercise in motor skill
   learning?},
Journal = {FRONTIERS IN PSYCHOLOGY},
Year = {2022},
Volume = {13},
Month = {OCT 18},
Abstract = {Parallel to the tremendous growth and expansion of video technology, it
   is easy and enjoyable for students to create a video as a learning
   activity. However, most previous studies primarily focused on
   declarative knowledge learning (e.g., language learning, science
   learning) rather than motor skill learning. The current study aimed to
   investigate whether creating and sharing a video with classmates would
   be more effective than merely creating a video and self-exercise to
   learn a motor skill in terms of intrinsic motivation, perseverance in
   learning, learning satisfaction, and roller-skating skill. Partially
   consistent with our hypothesis, we found that creating and sharing a
   video with classmates increased students' intrinsic motivation,
   perseverance in motor tasks, and learning satisfaction, but not
   roller-skating skill, followed by merely creating a video and then
   self-exercise. The findings have an important implication for motor
   skills learning: during teaching motor skills, teachers can use
   encourage students to create and share a video with classmates as a
   homework activity to increase students' intrinsic motivation,
   perseverance in motor tasks, and learning satisfaction.},
