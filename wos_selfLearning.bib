@article{ WOS:000730396100010,
Author = {Geertshuis, Susan and Liu, Qian and Rix, Ngaire and Murdoch, Odette and
   McConnell, Mark},
Title = {Learning by watching others learn: the use of videoed tutorials in
   undergraduate business education},
Journal = {HIGHER EDUCATION PEDAGOGIES},
Year = {2021},
Volume = {6},
Number = {1},
Pages = {156-174},
Month = {JAN 1},
Abstract = {The Fourth Industrial Age (4IA) is likely to be accompanied
   simultaneously by an increase in technology-mediated learning and an
   urgent need for people to leam rapidly, effectively and collaboratively.
   This study investigates the potential of vicarious teaming from videoed
   tutorials as a pedagogical tool suitable for the challenges of 4IA.
   Undergraduate Business students observed videos of student tutees
   responding to tutor prompts as they tackled openended and conceptually
   challenging problems. The results revealed that student observers
   self-reported: gains in their conceptual understanding from watching
   videoed tutorials; preferences for watching tutorial dialogues over
   alternative learning methods; and that watching videoed tutorials had
   positive impacts on their affect and access to additional
   learning-related information. The study concludes that vicarious teaming
   from videoed tutorials is an accessible technology-mediated pedagogy
   that is achievable by mainstream educators and is effective in
   developing conceptual understanding, engaging students and providing
   access to additional learning-related information.},
