@article{ WOS:000279137800027,
Author = {Abdous, M'hammed and Yoshimura, Miki},
Title = {Learner outcomes and satisfaction: A comparison of live video-streamed
   instruction, satellite broadcast instruction, and face-to-face
   instruction},
