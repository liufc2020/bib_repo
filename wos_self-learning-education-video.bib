@inproceedings{ WOS:000649266100118,
Author = {Reher, V and Rehbein, G. and Reher, P.},
Editor = {VanToi, V and Le, TQ and Ngo, HT and Nguyen, TH},
Title = {Integrating Video Recording and Self-reflection to Enhance Communication
   Skills Training for Dental Students},
Booktitle = {7TH INTERNATIONAL CONFERENCE ON THE DEVELOPMENT OF BIOMEDICAL
   ENGINEERING IN VIETNAM (BME7): TRANSLATIONAL HEALTH SCIENCE AND
   TECHNOLOGY FOR DEVELOPING COUNTRIES},
