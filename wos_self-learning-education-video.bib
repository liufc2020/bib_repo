@inproceedings{ WOS:000562759305006,
Author = {Hernandez-Saz, J. and Reyes, D. F. and Sales, D. L.},
Editor = {Chova, LG and Martinez, AL and Torres, IC},
Title = {THE USE OF AUDIO-VISUAL MATERIAL PRODUCED BY STUDENTS AS A TOOL IN THE
   LEARNING PROCESS},
Booktitle = {11TH INTERNATIONAL CONFERENCE OF EDUCATION, RESEARCH AND INNOVATION
   (ICERI2018)},
Series = {ICERI Proceedings},
Year = {2018},
Pages = {4926-4931},
Note = {11th Annual International Conference of Education, Research and
   Innovation (ICERI), Seville, SPAIN, NOV 12-14, 2018},
Abstract = {New learning environments drive the implementation of innovative
   strategies in agreement with the new educational model of the European
   Higher Education Area. Accordingly, an innovative exercise to promote
   cross-curricular competencies such as critical thinking, collaborative
   learning and improvement of the information and communication
   technologies and curricular specific competencies is presented in this
   paper. Students produce videos with their proposed solution of a problem
   from the teacher-selected problem sheet, which they have to modify and
   self-resolve. Videos are cell phone recorded and show the students
   explaining their proposed problem solution. The video performances
   indicate that the students were able to increase its motivation.
   Academic final results show that 65.4\% of students showed greater
   interest in the subject after of exercise and its participation rise
   compare to other traditional exercises. The survey answered by the
   students involved in this project points out that cell phone video
   recording media was a useful activity and they consider that the task
   was transferable to other courses. The use of the mobile phone as a
   pedagogical element is a reality today. And it gives us the possibility
   to teach digital competence as well as to increase the motivation and
