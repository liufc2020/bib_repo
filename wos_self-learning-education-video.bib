@article{ WOS:000242609300002,
Author = {Perry, Nancy E. and Winne, Philip H.},
Title = {Learning from learning kits: gStudy traces of students' self-regulated
   engagements with computerized content},
Journal = {EDUCATIONAL PSYCHOLOGY REVIEW},
Year = {2006},
Volume = {18},
Number = {3},
Pages = {211-228},
Month = {SEP},
Abstract = {Researching self-regulated teaming (SRL) as a process that evolves
   across multiple episodes of studying poses large methodological
   challenges. While self-report data provide useful information about
   learners' perceptions of teaming, these data are not reliable indicators
   of studying tactics learners actually use while studying, especially
   when learners are young children. We argue that self-reports about SRL
   need to be augmented by fine-grained traces that are records of
   learners' actual activities as they study. We describe how gStudy
   software unobtrusively collects detailed trace data about learners' use
   of study tactics as they engage with content presented in teaming
   kits-collections of docurnents (e.g., texts, graphics, video clips) and
   tasks (e.g., notes, concept maps) on which learners operate to study. We
   suggest that trace data can advance research about how learners select,
   monitor, assemble, rehearse, and translate information to learn it, and
   provide raw materials for mapping SRL and its effects. Examples from the
   Lift Cycles Learning Kit that supports grade I students teaming about
   the life cycles of humans and frogs are given.},
