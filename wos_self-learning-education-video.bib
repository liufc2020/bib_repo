@inproceedings{ WOS:000377304001132,
Author = {Soares, Filomena and Lopes, Ana Paula and Vieira, Isabel},
Editor = {Chova, LG and Martinez, AL and Torres, IC},
Title = {DESIGNING VIDEO LECTURES FOR MOOC},
