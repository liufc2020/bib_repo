@inproceedings{ WOS:001178562003147,
Author = {Yu, Wenhan and Zhao, Jun},
Book-Group-Author = {IEEE},
Title = {Heterogeneous 360 Degree Videos in Metaverse: Differentiated
   Reinforcement Learning Approaches},
Booktitle = {IEEE CONFERENCE ON GLOBAL COMMUNICATIONS, GLOBECOM},
Series = {IEEE Global Communications Conference},
Year = {2023},
Pages = {3336-3341},
Note = {IEEE Conference on Global Communications (IEEE GLOBECOM) - Intelligent
   Communications for Shared Prosperity, Kuala Lumpur, MALAYSIA, DEC 04-08,
   2023},
Organization = {IEEE; Huawei; Technol Innovat Inst; ZTE; Intel; Rohde \& Schwarz; NL;
   VLAVI},
Abstract = {Advanced video technologies are driving the development of the
   futuristic Metaverse, which aims to connect users from anywhere and
   anytime. As such, the use cases for users will be much more diverse,
   leading to a mix of 360-degree videos with two types: non-VR and VR 360.
   videos. This paper presents a novel Quality of Service model for
   heterogeneous 360. videos with different requirements for frame rates
   and cybersickness. We propose a frame-slotted structure and conduct
   frame-wise optimization using self-designed differentiated deep
   reinforcement learning algorithms. Specifically, we design two
   structures, Separate Input Differentiated Output (SIDO) and Merged Input
   Differentiated Output (MIDO), for this heterogeneous scenario. We also
   conduct comprehensive experiments to demonstrate their effectiveness.},
Publisher = {IEEE},
Address = {345 E 47TH ST, NEW YORK, NY 10017 USA},
Type = {Proceedings Paper},
