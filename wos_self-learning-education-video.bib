@article{ WOS:000783270600001,
Author = {Negi, Shivsevak and Mitra, Ritayan},
Title = {Native language subtitling of educational videos: A multimodal analysis
   with eye tracking, EEG and self-reports},
Journal = {BRITISH JOURNAL OF EDUCATIONAL TECHNOLOGY},
Year = {2022},
Volume = {53},
Number = {6},
Pages = {1793-1816},
Month = {NOV},
Abstract = {Language assistance becomes an integral component of the
   teaching-learning process in -non-English-speaking multilingual
   societies where education takes place predominantly in local languages.
   Subtitles have been found to be beneficial in facilitating understanding
   of the English language medium media in such situations. However, when
   accompanied with subtitles, information-rich instructional materials can
   add to the learner's cognitive load as their attention must be split
   between the content and the subtitle. To evaluate the cognitive and
   affective impacts of subtitle language on learning, we conducted a
   multimodal study using eye tracking, electroencephalogram (EEG),
   self-report, and pre-post test data from 51 individuals watching a
   12-minute educational video in those who had no subtitles, those who had
   English subtitles (L2) and those who had subtitles in their native
   language (L1). We discovered (a) positive learning gains for groups with
   subtitles, whether L1 or L2, which suggested positive impact of
   subtitles on learning outcome; (b) native language subtitling yielded
   the highest instructional efficiency and supported effective
   distribution of visual attention between slide content and subtitle; (c)
   native language subtitles elicited the lowest cognitive load, as
   evidenced by both EEG measurements and self-report data; and (d) gaze
   data highlighted key strategies of high and low performers during
   interaction with subtitled media.},
