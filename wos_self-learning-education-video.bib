   North, Chris, Univ Canterbury, Sch Hlth Sci, Christchurch, New Zealand.},
DOI = {10.1080/17408989.2016.1268586},
ISSN = {1740-8989},
EISSN = {1742-5786},
Keywords = {Teacher education; reframing; self-study; reflection; physical
   positioning},
Keywords-Plus = {PEDAGOGY},
