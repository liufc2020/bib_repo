@article{ WOS:000657571100001,
Author = {Scholes, Laura and Mills, Kathy A. and Wallace, Elizabeth},
Title = {Boys' gaming identities and opportunities for learning},
Journal = {LEARNING MEDIA AND TECHNOLOGY},
Year = {2022},
Volume = {47},
Number = {2},
Pages = {163-178},
Month = {APR 3},
Abstract = {This article addresses a gap in research about primary school boys'
   identification as `gamers.' Drawing on a survey of 318 Year 3 (7-8 years
   old) students, the research identified boys' self-reported enjoyment for
   gaming, their frequency using digital devices, and their self-rated
   digital skills. Interviews with four boys from the survey also explored
   the lifeworlds of self-professed `gamers.' Findings point to the
   salience of games for many boys' emerging identities and the
   inter-related nature of their experiences. We argue that teachers can
   capitalise on the strength of video games to create `in-group' cultures
   and communities of practice in their classrooms to support learning.},
