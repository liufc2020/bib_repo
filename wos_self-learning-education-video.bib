Author = {Salleh, Noor Azlina Mohd and Aris, Sharipah Ruzaina Syed and Ismail,
   Azianti and Anuar, Megat Iskandar Shah Megat Shahrul and Manan, Ahmad
   Naqib Akmal Abdul},
Title = {Gamification Approach for Interactive Learning of Lean Manufacturing
   Activities},
