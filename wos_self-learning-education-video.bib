@article{ WOS:000492834500004,
Author = {Hefter, Markus H. and ten Hagen, Inga and Krense, Claudia and Berthold,
   Kirsten and Renkl, Alexander},
Title = {Effective and Efficient Acquisition of Argumentation Knowledge by
   Self-Explaining Examples: Videos, Texts, or Graphic Novels?},
Journal = {JOURNAL OF EDUCATIONAL PSYCHOLOGY},
Year = {2019},
Volume = {111},
Number = {8},
Pages = {1396-1405},
Month = {NOV},
Abstract = {A rising number of (online) learning scenarios feature video-based
   worked examples. We analyzed the effects of the presentation mode of
   worked examples on their effectiveness and efficiency in two experiments
   with university students (N-1 = 57; N-2 = 43). The students acquired
   argumentation knowledge by self-explaining different types of worked
   examples. In Experiment 1, we compared video-based examples with written
   examples, and in Experiment 2, we compared video-based examples with
   graphic novel-based examples. Regardless of the examples' presentation
   mode, we observed similar learning processes (i.e., invested mental
   effort and self-explanation quality) as well as a large effect on
   learning outcomes (i.e., conceptual knowledge about argumentative
   principles). Although they appear similarly effective, we found learning
   by self-explaining written examples and by self-explaining graphic
   novels to be less time consuming and, thus, more efficient than learning
   by self-explaining video examples.},
