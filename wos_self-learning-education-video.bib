Title = {Self-adapted Frame Selection Module: Refine the Input Strategy for Video
   Saliency Detection},
Booktitle = {ALGORITHMS AND ARCHITECTURES FOR PARALLEL PROCESSING, ICA3PP 2021, PT II},
Series = {Lecture Notes in Computer Science},
Year = {2022},
Volume = {13156},
