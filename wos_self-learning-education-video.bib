@inproceedings{ WOS:001290109600014,
Author = {Nakao, Ryoma and Nakajima, Tatsuo},
Editor = {Zaphiris, P and Ioannou, A},
Title = {The Effect of Changes in the Environment in a Virtual Space on Learning
   Effectiveness},
Booktitle = {LEARNING AND COLLABORATION TECHNOLOGIES, LCT 2023, PT II},
Series = {Lecture Notes in Computer Science},
Year = {2023},
Volume = {14041},
Pages = {199-211},
Note = {10th International Conference on Learning and Collaboration Technologies
   (LCT) Held as Part of the 25th International Conference on
   Human-Computer Interaction (HCII), Copenhagen, DENMARK, JUL 23-28, 2023},
Abstract = {In this study, we measured the effects of learning in different
   environments for lecture-style learning using videos and
   self-study-style learning on memorizing English vocabulary. As elements
   of the environment, we conducted experiments on the complexity of the
   surrounding environment, the presence of avatars, and colors and
   confirmed the differences in learning effects under these environments.
   The results confirmed that in the case of lecture-based learning, it was
   not clear whether the surrounding environment had any effect. In the
   case of self-study-type learning, it was confirmed that the difference
   in the surrounding environment affected the learning effect. With regard
