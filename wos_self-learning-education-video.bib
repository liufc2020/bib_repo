@inproceedings{ WOS:000455083200026,
Author = {Silverajah, V. S. Giita and Govindaraj, Anandraj},
Book-Group-Author = {Assoc Comp Machinery},
Title = {The use of digitized self-learning resources to enhance students'
   independent learning},
Booktitle = {PROCEEDINGS OF THE 9TH INTERNATIONAL CONFERENCE ON EDUCATION TECHNOLOGY
   AND COMPUTERS (ICETC 2017)},
Year = {2017},
Pages = {131-135},
Note = {9th International Conference on Education Technology and Computers
   (ICETC), Barcelona, SPAIN, DEC 20-22, 2017},
Organization = {IACSIT},
Abstract = {Higher educational institutions aim to develop students who take
   responsibility of their own learning. Such students can manage and
   assess their own learning and they often perform better than others.
   Experts believe that one way to develop independent learners are through
   directed independent learning activities which students are required to
   complete with some or minimal supervision from instructors. This paper
   investigates students' perceptions on the use of digitized self-learning
   resources in enhancing students. independent learning. The participants
   of this study were 37 Chemistry students enrolled in the Australian
   Matriculation (AUSMAT) Programme at Sunway College, Malaysia. As part of
   their directed independent learning, participants were provided with
   paper-based self-learning material containing a compilation of
   examination questions on Oxidation and Reduction (Redox) chapter,
   marking key and solution videos provided in a QR code, as well as,
   similar past year questions for practice. Data collection tools were a
   survey questionnaire which consisted of 5-point Likert scale and
   open-ended questions and teacher reflective notes. Analysis of data
   revealed that even though most students favoured the use of digitized
   self-learning resources to support their learning, some were reluctant
   and preferred the use of more traditional methods of learning. The study
   provides valuable insights into the importance of preparing students for
   independent learning and the challenges in preparing digitized
   self-learning resources that would encourage students to become
   autonomous learners.},
