@article{ WOS:001002282500001,
Author = {Hamilton, Kelly L. and Kuo, Yu-Chun and Horneffer, Peter and Stein, T.
   Peter and Goldberg, Gary S.},
Title = {Video Didactic Preparation Augments Problem-Based Learning for First
   Year Medical Students},
Journal = {JOURNAL OF MEDICAL EDUCATION AND CURRICULAR DEVELOPMENT},
Year = {2023},
Volume = {10},
Month = {MAY},
Abstract = {Problem-based learning (PBL) utilizes a self-directed strategy. This
   process relies on group participation to succeed. Students without a
   background in biology or medicine can feel overwhelmed by the complexity
   of the subject matter and unable to participate in the group learning
   process. We incorporated curated educational videos in the PBL
   curriculum to help address this situation. First year medical students
   participated in this study in the form of a typical PBL session. They
   were then assessed on basic and clinical science knowledge and their
   learning experience. Student basic science and clinical knowledge were
   similar between the student groups. However, the students given a list
   of suggested videos scored higher in their learning experience,
   perception of feeling prepared, and participating in the group PBL
   experience than students who were not given the video list. Results from
   this study indicate that videos can be utilized to enhance the PBL
   process.},
