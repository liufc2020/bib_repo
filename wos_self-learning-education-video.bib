@article{ WOS:000744818400001,
Author = {Omarchevska, Yoana and Lachner, Andreas and Richter, Juliane and
   Scheiter, Katharina},
Title = {Do Video Modeling and Metacognitive Prompts Improve Self-Regulated
   Scientific Inquiry?},
Journal = {EDUCATIONAL PSYCHOLOGY REVIEW},
Year = {2022},
