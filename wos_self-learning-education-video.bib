@article{ WOS:000898036100001,
Author = {Scholes, Laura and Rowe, Luke and Mills, Kathy A. and Gutierrez, Amanda
   and Pink, Elizabeth},
Title = {Video gaming and digital competence among elementary school students},
Journal = {LEARNING MEDIA AND TECHNOLOGY},
Year = {2024},
Volume = {49},
Number = {2},
Pages = {200-215},
Month = {APR 2},
Abstract = {Engagement with video games can potentially advance student digital
   competence however, there is a digital skills gap by the time young
   people progress into adolescence. This current research explores how
   elementary school students' digital self-efficacy might relate to
   experiences in video game environments to influence perceptions of
   digital competence. We examine the differential impact of sex,
   self-efficacy, and socioeconomic status (SES) on 7-10-year-old students'
   (N = 613) perceptions of video gaming and their digital skills. Analysis
   revealed the unexpected finding that SES was inversely related to
   enjoyment for gaming and digital technology, with students in the
   lower-SES category responding more positively compared to students in
   higher SES categories. As expected, boys self-reported digital skills
   higher than girls across all SES categories. We argue for the use of
   gaming pedagogies to support learning in classrooms that accounts for
   nuances in students' digital self-efficacy moderated by gender and SES.},
