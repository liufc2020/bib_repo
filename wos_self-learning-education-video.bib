@article{ WOS:000758955100003,
Author = {Bailey, Daniel R. and Almusharraf, Norah and Almusharraf, Asma},
Title = {Video conferencing in the e-learning context: explaining learning
   outcome with the technology acceptance model},
Journal = {EDUCATION AND INFORMATION TECHNOLOGIES},
