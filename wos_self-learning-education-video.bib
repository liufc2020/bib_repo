@article{ WOS:001185841900001,
Author = {Millea, Danielle Jayne},
Title = {360° Video as a learning tool for filmmaking},
Journal = {INTERACTIVE LEARNING ENVIRONMENTS},
Year = {2024},
Month = {2024 MAR 15},
Abstract = {Filmmaking is an inherently practical area of study. Not all students
   can access film school because of monetary, location and health issues.
   Options without schooling are online in the form of tutorial videos. 360
   degrees video offers a more interactive approach than traditional
   learning tools, and the technology can enhance learning and promote
   self-guided learning. Using constructivist grounded theory methodology
   through cyclic testing of mixed method data collection via six
   participant survey rounds, 360 degrees video content was created and
   hosted on the online hub www.filmschool360.com (
   https://dmillea2662.Wixsite.com/framework/). A technical workflow was
   developed to show that 360 degrees technology can aid practical
   filmmaking study. Filmmaking students, alumni, academics, technical
   staff, and professionals tested the videos and found them to be
   interesting, informative, and immersive. Audio, direction, and video
   elements were found to be important. One 360 degrees video has the
   potential for freedom of learning through unique viewpoints while
   reducing creator costs. Short five-minute videos can provide an
   accessible learning method for those with disabilities, with no barriers
   towards gender or ethnicity. The freedom to navigate the videos empowers
   the learner, enhancing their self-guided learning skills. The bridge
   between traditional and online learning can be opened using 360 degrees
   video.},
